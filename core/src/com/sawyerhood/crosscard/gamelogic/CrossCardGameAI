package com.sawyerhood.crosscard.gamelogic;

import java.util.Collection;

public class CrossCardGameAI {
  
  public CrossCardGameAI() {
  }
  
  public turn(CrossCard cardToPlay, CrossCardGameBoard currentBoard, CrossCardDeck currentDeck, ) {
    int[][] moves = new int[3][3];
    while (time > 0) {
      for (int i=0; i < 3; i++) {
        for (int j=0; j < 3; j++) {
          if (!currentBoard.isOccupied(i, j)) {
            moves[i][j] += simulate(i, j, currentBoard, currentDeck)
          }
        }
      }
    }
  }
}
